{
    "name":  "fastify-di-boilerplate",
    "version":  "1.0.0",
    "main":  "dist/server.js",
    "scripts":  {
                    "start":  "node dist/server.js",
                    "db:generate":  "drizzle-kit generate --config drizzle.config.ts",
                    "db:migrate":  "drizzle-kit migrate --config drizzle.config.ts",
                    "build":  "tsc -p tsconfig.json",
                    "lint":  "biome lint src tests README.md",
                    "format":  "biome format src tests README.md",
                    "test":  "jest",
                    "lint:fix":  "biome lint --apply src tests README.md",
                    "dev":  "tsx watch src/server.ts"
                },
    "keywords":  [

                 ],
    "author":  "",
    "license":  "ISC",
    "description":  "Fastify boilerplate with awilix DI, Drizzle ORM, Zod, Swagger, Biome, and Jest",
    "dependencies":  {
                         "@fastify/awilix":  "^8.0.0",
                         "@fastify/swagger":  "^9.5.2",
                         "@fastify/swagger-ui":  "^5.2.3",
                         "awilix":  "^12.0.5",
                         "dotenv":  "^17.2.2",
                         "drizzle-orm":  "^0.44.5",
                         "fast-glob":  "^3.3.3",
                         "fastify":  "^5.6.1",
                         "fastify-plugin":  "^5.0.1",
                         "pg":  "^8.16.3",
                         "pino":  "^9.11.0",
                         "zod":  "^4.1.11"
                     },
    "devDependencies":  {
                            "@biomejs/biome":  "^2.2.4",
                            "@types/jest":  "^30.0.0",
                            "@types/node":  "^24.5.2",
                            "@types/pg":  "^8.15.5",
                            "drizzle-kit":  "^0.31.5",
                            "jest":  "^30.1.3",
                            "ts-jest":  "^29.4.4",
                            "tsx":  "^4.20.6",
                            "typescript":  "^5.9.2"
                        }
}